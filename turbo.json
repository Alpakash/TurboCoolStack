{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "ui#build": {
      "cache": false
    },
    "docs#build": {
      "cache": false,
      "dependsOn": ["^ui#build"]
    },
    "web#build": {
      "cache": false,
      "dependsOn": ["^ui#build"]
    },
    "electronrenderer#build": {
      "cache": false,
      "dependsOn": ["^ui#build"]
    },
    "electronmain#build": {
      "cache": false,
      "dependsOn": ["^ui#build", "^electronrenderer#build"]
    },
    "electronbuilder#build": {
      "cache": false,
      "dependsOn": [
        "^ui#build",
        "^electronrenderer#build",
        "^electronmain#build"
      ]
    },
    "build": {
      "outputs": [
        "dist/**",
        "dist-main/**",
        "dist-renderer/**",
        "build/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "clean": {
      "cache": false
    }
  }
}
